{
  "name": "react-native-leaflet-ts",
  "version": "0.3.1",
  "description": "React Native Leaflet with Typescript support",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "keywords": [
    "react-native",
    "leaflet",
    "react-native-leaflet",
    "RN-leaflet",
    "map",
    "tiles",
    "react",
    "maps",
    "openstreetmap",
    "typescript",
    "react-native-indoor-map",
    "weapp",
    "rn leaflet",
    "we app"
  ],
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-leaflet-ts.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepare": "bob build",
    "release": "release-it"
  },
  "repository": "https://github.com/putteabrahamsson/react-native-leaflet-ts",
  "author": "Patrick Abrahamsson <patrick@weapp.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/putteabrahamsson/react-native-leaflet-ts/issues"
  },
  "homepage": "https://github.com/putteabrahamsson/react-native-leaflet-ts#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@react-native-community/eslint-config": "^3.0.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^28.1.2",
    "@types/react": "18.0.21",
    "@types/react-native": "0.70.4",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "0.70.2",
    "react-native-builder-bob": "^0.18.3",
    "react-native-webview": "11.25.0",
    "release-it": "^15.0.0",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "@types/react": "18.0.21",
    "@types/react-native": "0.70.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "@babel/eslint-parser",
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "bootstrap": "^5.2.3"
  }
}
